# include <stdio.h>
int main()
{
    int mark;
    printf("Enter the marks: ");
    scanf("%d",&mark);
    switch(mark)
    {
        case 90 ... 100:
        printf("Grade: S");
        break;
        case 80 ... 89:
        printf("Grade: A");
        break;
        case 70 ... 79:
        printf("Grade: B");
        break;
        case 60 ... 69:
        printf("Grade: C");
        break;
        case 50 ... 59:
        printf("Grade: D");
        break;
        default:
        printf("Grade: F");
    }
    return 0;
}
